{
  // Required
  "manifest_version": 2,
  "name": "Tab Through",
  "version": "1.0",

  // Recommended
  "description": "Add the ability to tab through the heading tags of an html page! Good for reading documentation, long articles, ...",
  "icons": {"16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"},

  // Optional
  "author": "Dominic Spadacene",

  "background" :{
    "scripts": ["background.js"]
  },

  "browser_action":{
    "default_icon": "icon16.png",
    "default_title": "Make readable!"
  },

  "permissions": [
    "activeTab"
  ],

  "commands": {
    "Execute Tab Through": {
      "suggested_key": {
        "default": "Ctrl+Shift+5",
        "mac": "Command+Shift+5"
      },
      "description": "Execute Tab Through "
    }
  }
 
}